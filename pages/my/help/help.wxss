.main {
    width: 100%;
    height: 100%;
    background-color: #F5F5F5;
    padding: 20rpx;
    box-sizing: border-box;
}

.content {
    border-radius: 12rpx;
    background: white;
    padding: 24rpx 20rpx;
    box-sizing: border-box;
}

.header {
    margin-bottom: 30rpx;
    text-align: center;
}

.text {
    color: #999;
    font-size: 24rpx;
    margin-bottom: 30rpx;
    line-height: 38rpx;
}

.item .title {
    color: #FFAB51;
    font-size: 24rpx;
    margin-bottom: 30rpx;
}