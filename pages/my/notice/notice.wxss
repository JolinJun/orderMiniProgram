.main {
    width: 100%;
    height: 100%;
    background-color: #F5F5F5;
    padding: 20rpx;
    box-sizing: border-box;
}

.content {
    border-radius: 12rpx;
    background: white;
    padding: 24rpx 20rpx;
    box-sizing: border-box;
}

.header {
    margin-bottom: 30rpx;
    text-align: center;
}

.padding-left {
    padding-left: 20rpx;
}

.text {
    color: #999;
    font-size: 24rpx;
    margin-bottom: 30rpx;
    line-height: 38rpx;
    position: relative;
}

.item .title {
    color: #FFAB51;
    font-size: 24rpx;
    margin-bottom: 30rpx;
}

.item .em {
    width: 14rpx;
    height: 14rpx;
    border-radius: 50%;
    background: #999;
    position: absolute;
    left: 0;
    top: 10rpx;
    display: block;
}